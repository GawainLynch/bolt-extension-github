{
    "name": "bolt/github",
    "description": "GitHub API interface for Bolt",
    "type": "bolt-extension",
    "keywords": [
        "GitHub",
        "API",
        "Webhooks"
    ],
    "require": {
        "bolt/bolt": "^3.0",
        "ext-curl": "*",
        "knplabs/github-api": "^1.6"
    },
    "provide": {
        "symfony/event-dispatcher": "2.8"
    },
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Gawain Lynch",
            "email": "gawain.lynch@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Bolt\\GitHub\\": "src"
        }
    },
    "extra": {
        "bolt-class": "Bolt\\Extension\\Bolt\\GitHub\\GitHubExtension",
        "bolt-icon": "icon.png",
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
